{
	"name": "moare/moare-basic",
	"license": "GPL-2.0-or-later",
	"description": "Functions Studio Moare use to create a websites base on WordPress",
	"homepage": "https://github.com/antoniofelices/moare-basic",
	"keywords": [ "wordpress", "plugin", "custom post types", "custom taxonomies" ],
	"support": {
		"issues": "https://github.com/antoniofelices/moare-basic"
	},
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
		"squizlabs/php_codesniffer": "3.7.1",
		"wp-coding-standards/wpcs": "~2.3.0",
		"phpcompatibility/phpcompatibility-wp": "~2.1.4",
		"yoast/phpunit-polyfills": "^1.0.4"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
